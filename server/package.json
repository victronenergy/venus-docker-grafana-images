{
  "name": "venus-docker-grafana-server",
  "version": "2.4.0",
  "description": ".",
  "main": "index.js",
  "scripts": {
    "start": "node bin/venus-server",
    "format": "prettier-standard lib/**/*.js* index.js",
    "heroku-postbuild": "npm install @signalk/simple-gpx",
    "lint": "eslint lib/ --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "webpack -d --progress --watch --profile --json > compilation-stats.json --env.dev",
    "build": "webpack -p --progress --env.prod",
    "clean": "rimraf ./build",
    "start-web": "webpack-dev-server --progress --colors --inline --env.dev",
    "create-release": "github-create-release --owner victronenergy --repository venus-docker-grafana-images",
    "release": "git tag -d v$npm_package_version; git tag v$npm_package_version && git push --tags && git push && npm run create-release"
  },
  "lint-staged": {
    "linters": {
      "lib/*.js*": [
        "prettier-standard",
        "git add"
      ],
      "index.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "bin": {
    "venus-server": "./bin/venus-server"
  },
  "author": "Scott Bender <scott@scottbender.net>",
  "license": "MIT",
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "body-parser": "^1.14.1",
    "debug": "^4.1.0",
    "express": "^4.10.4",
    "express-namespace": "^0.1.1",
    "influx": "^5.0.7",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mqtt": "^2.18.8",
    "node-fetch": "^2.5.0",
    "node-ssdp": "^4.0.0",
    "primus": "^7.0.0",
    "react-markdown": "^4.0.3",
    "tsscmp": "^1.0.6",
    "winston": "^3.2.1",
    "ws": "^6.1.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "bootstrap": ">=4.1.2",
    "chart.js": "2.9.4",
    "copy-webpack-plugin": "4.2.1",
    "css-hot-loader": "1.3.3",
    "css-loader": "0.28.7",
    "eslint": "^5.6.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-restricted-globals": "^0.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "font-awesome": "4.7.0",
    "history": "4.7.2",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "husky": "^1.0.0",
    "lint-staged": "^8.0.4",
    "node-sass": "4.7.1",
    "prettier-standard": "^8.0.0",
    "react": "16.1.1",
    "react-chartjs-2": "2.6.4",
    "react-dom": "16.1.1",
    "react-jsonschema-form-bs4": "^1.0.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "4.2.2",
    "react-transition-group": "2.2.1",
    "reactstrap": "^5.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "simple-line-icons": "2.4.1",
    "source-list-map": "2.0.0",
    "style-loader": "0.19.0",
    "uglify-js": "3.1.10",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/victronenergy/venus-docker-grafana-images.git"
  },
  "bugs": {
    "url": "https://github.com/victronenergy/venus-docker-grafana-images/issues"
  }
}
